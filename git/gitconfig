[alias]
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
    uno = status -uno
    subclean = submodule foreach "git clean -xdf"
    subfetch = submodule foreach "git fetch -p"
    subpull = submodule foreach "git pull"
    subcomaster = submodule foreach "git checkout master"
[color]
    branch = auto
    diff = auto
    interactive = auto
    status = auto
